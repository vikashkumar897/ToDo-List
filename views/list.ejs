<%- include("header") -%> 
    <div class="box" id="heading">
        <h1><%= listTitle %> </h1>
    </div>

    <div class="box">
        <% for (i=0;i<toDoNewItem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= toDoNewItem[i] %> </p>                
            </div>
        <% } %>

        <form class="item" action="/" method="post">
            <input type="text" name="toDoItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value="<%= listTitle %> ">+</button>
        </form>
        
    </div>
    <div class="box1"><a href=<%= linkRefer %>  class="BUTTON_WRB">
        <%= btn_text %></a></div>
    


<%- include("footer") -%>